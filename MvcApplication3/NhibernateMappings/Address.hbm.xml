<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping  xmlns="urn:nhibernate-mapping-2.2">
  <class name="NhibernateMappings.Address, MvcApplication3" table="Address" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Address1">
      <column name="Address1" sql-type="varchar" not-null="false" />
    </property>
    <property name="Address2">
      <column name="Address2" sql-type="varchar" not-null="false" />
    </property>
    <property name="City">
      <column name="City" sql-type="varchar" not-null="false" />
    </property>
    <property name="State">
      <column name="State" sql-type="varchar" not-null="false" />
    </property>
    <property name="Zipcode">
      <column name="ZipCode" sql-type="varchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>